    <script type="text/javascript">
    let Idd;
    $(document).ready(function(){
        console.log('test');
        $('.editBtns').on('click', function(e){
            let id = $(e.currentTarget).parent().parent().attr("id")
            Idd = $(e.currentTarget);
            $.ajax({
              type : 'GET',
              url : '/admin/usersmanagement/update/' + id,
              success: function(data){
                  // insert data values to modal
                  
                  $('#myModals')
                  .find('#email1').val(data.email).end()
                  .find('#phone1').val(data.phone).end()
                  .find('#education1').val(data.education).end()
                  .find('#aoi1').val(data.aoi).end()
                  .find('#name1').val(data.name).end()
                  .find('#level1').val(data.level).end()
                  .find('#status1').val(data.status).end();

                // $("#loginDiv").html(data);
              }
            });
        });
        $("#editBtnModals").click(function(){
            var id = Idd.parent().parent().attr("id");
            var email  = $("#email1").val();
            var phone = $("#phone1").val();
            var education = $("#education1").val();
            var aoi = $("#aoi1").val();
            var name = $("#name1").val();
            var level = $("#level1").val();
            var status = $("#status1").val();
                      
            var Data ={'email': email, 'phone': phone, 'education': education, 'aoi': aoi, 'name': name,'level': level, 'status': status};
            $.ajax({
              type : 'put',
              url : '/admin/usersmanagement/edit/' + id,
              data : Data,
              success: function(data){
                // $("#regDiv").html(data);
                alert('success');
                window.location.href = '/admin';
              }
            });
        });
        $("#loginBtn").click(function(){
            
            $.ajax({
              type : 'GET',
              url : '/login',
              success: function(data){
                $("#loginDiv").html(data);
              }
            });
        });
        //=====Login Form Request=============================================
        $("#loginForm").click(function(){
          var uname  = $("#uname").val();
          var upass = $("#upass").val();
          var loginData ={'name': uname,'pass':upass};
          $.ajax({
              type : 'POST',
              url : '/demo',
              data : loginData,
              success: function(data){
              $("#mainDiv").html(data);
              }
            });
        });
     //=====Register Form=============================================
        $("#regForm").click(function(){
          var uname  = $("#uname").val();
          var upass = $("#upass").val();
          var regData ={'name': uname,'pass':upass};
            $.ajax({
              type : 'POST',
              url : '/regiterToDb',
              data : regData,
              success: function(data){
              $("#mainDiv").html(data);
              }
            });
        });
     //Save profile Data================================================
     $('#saveBtn').click(function(){
       var email = $("#email").val();
       var phone = $("#phone").val();
       var education = $("#education").val();
       var aoi = $("#aoi").val();
       var name = $("#name").val();
       var pass = $("#pass").val();
       var profileData = {'email':email,'phone':phone,'education' : education,'aoi':aoi,'name' : name,'pass' : pass};
       $.ajax({
         type : 'POST',
         url : '/completeprofile',
         data : profileData,
         success : function(data){
            $("#mainDiv").html(data);
         }
       });
     });
     });
     </script>

